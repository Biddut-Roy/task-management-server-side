{
    "version": 2,
    "name": "Task Management",
    "builds": [
       { 
        
       "src": "index.js",
        "use": "@vercel/node"
     }
    ],
    "routes": [
       { 
         "src": "/(.*)",
         "dest": "/",
         "methods": ["GET", "OPTIONS" , "POST", "PATCH", "PUT", "DELETE"]
        }
    ]
 }